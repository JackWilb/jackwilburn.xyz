{
  "name": "jackwilburn.xyz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "serve": "vite --host 0.0.0.0 --port 8080",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --no-fix src",
    "lint:fix": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "@vitejs/plugin-vue": "^3.0.1",
    "autoprefixer": "^9",
    "mini-css-extract-plugin": "^1.3.3",
    "postcss": "^8",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "vite": "^3.0.4",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/eslint-config-airbnb": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^8.0.0",
    "eslint-plugin-vuejs-accessibility": "^1.2.0",
    "lint-staged": "^10.5.3",
    "yorkie": "^2.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "repository": "https://github.com/JackWilb/jackwilburn.xyz",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint"
    ],
    "*.vue": [
      "yarn lint"
    ]
  }
}
